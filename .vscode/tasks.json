{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++ build active file",
      "command": "g++",
      "args": [
        "-std=c++20",
        "-O2",
        "-Wall",
        "-Wextra",
        "-I/opt/homebrew/opt/boost/include",
        "-L/opt/homebrew/opt/boost/lib",
        "-lboost_system",
        "-o",
        "kraken",
        "kraken.cpp",
        "-lboost_system",
        "-lboost_json",
        "-lssl",
        "-lcrypto",
        "-pthread"
      ],

      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
